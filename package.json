    "vue": "^3.2.25",
    "vue-router": "^4.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.2.1",
    "@testing-library/vue": "^6.6.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.5",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.17.1",
    "vue-tsc": "^0.34.7"
  }
}
